{
    "name": "ssr",
    "version": "1.0.0",
    "description": "Server Side Rendering with ReactJS, React Router v4, Redux",
    "main": "index.js",
    "scripts": {
        "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
        "lint": "yarn run lint-js --fix",
        "test": "jest",
        "test-update": "jest --updateSnapshot",
        "test-watch": "yarn run test --watch --notify",
        "test-cover": "yarn run test --coverage",
        "coverage": "yarn run test-cover && open coverage/lcov-report/index.html",
        "build-client-dev": "webpack-dev-server --env development --progress --color --hot --inline",
        "build-client-prod": "webpack --env production --progress --color",
        "start-server-dev": "nodemon ./src/server --exec babel-node",
        "start-server-prod": "babel-node ./src/server",
        "start-prod": "npm run build-client-prod && npm run start-server-prod"
    },
    "keywords": [
        "ssr",
        "reactjs",
        "redux",
        "react-router"
    ],
    "author": "Yury_Karalkou",
    "license": "ISC",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-jest": "^22.4.1",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-loader": "^0.28.9",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.16.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jest": "^21.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "^2.5.0",
        "eslint-plugin-react": "^7.6.1",
        "file-loader": "^1.1.8",
        "html-webpack-plugin": "^3.0.4",
        "jest": "^22.4.2",
        "nodemon": "^1.12.1",
        "prettier": "1.10.2",
        "pug": "^2.0.0-rc.4",
        "pug-loader": "^2.3.0",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.2.0",
        "style-loader": "^0.20.2",
        "webpack": "^3.11.0",
        "webpack-cli": "^2.0.9",
        "webpack-dev-server": "^2.11.2",
        "webpack-merge": "^3.0.0"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "bemto-components": "^1.7.0",
        "email-validator": "^1.1.1",
        "express": "^4.16.2",
        "history": "^4.7.2",
        "immutable": "^3.8.2",
        "isomorphic-fetch": "^2.2.1",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "redux": "^3.7.2",
        "redux-form": "^7.3.0",
        "redux-logger": "^3.0.6",
        "redux-saga": "^0.16.0",
        "reselect": "^3.0.1",
        "styled-components": "^3.1.6",
        "typeface-open-sans": "^0.0.54"
    }
}
